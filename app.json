{
   "expo": {
      "name": "Expensify",
      "slug": "expensify-app",
      "version": "1.2.0",
      "orientation": "portrait",
      "platforms": ["android"],
      "icon": "./assets/icon.png",
      "splash": {
         "image": "./assets/splash.png",
         "resizeMode": "contain",
         "backgroundColor": "#ffffff"
      },
      "updates": {
         "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
         "supportsTablet": true
      },
      "web": {
         "favicon": "./assets/favicon.png"
      },
      "android": {
         "package": "com.julioojeda.expensify",
         "versionCode": 1,
         "permissions": ["WRITE_EXTERNAL_STORAGE"]
      }
   }
}
